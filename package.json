{
  "name": "pique-site",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "7.3.0"
  },
  "scripts": {
    "start": "node ./server/dist/index.js",
    "clean-server": "rimraf ./server/dist",
    "clean-client": "rimraf ./build",
    "clean": "npm run clean-client && npm run clean-server",
    "build-server": "babel ./server/src -d ./server/dist",
    "build-client": "webpack --progress -p",
    "prebuild": "npm run clean",
    "build": "npm run build-client && npm run build-server",
    "postbuild": "node ./scripts/cross-browser-open.js ./build/index.html"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-polyfill": "^6.20.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "filepicker-js": "^2.4.18",
    "firebase": "^3.6.3",
    "nuka-carousel": "^2.0.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.5",
    "react-router": "4.0.0-alpha.6",
    "redux": "^3.6.0",
    "sanitize.css": "^4.1.0",
    "stripe": "^4.14.0"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.16.0",
    "image-webpack-loader": "2.0.0",
    "json-loader": "^0.5.4",
    "koa": "2.0.0-alpha.7",
    "postcss-cssnext": "^2.9.0",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^0.13.0",
    "postcss-reporter": "^1.4.1",
    "redux-logger": "^2.7.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.15",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.13.2",
    "write-file-webpack-plugin": "^3.3.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports",
      "transform-function-bind"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "license": "MIT"
}
